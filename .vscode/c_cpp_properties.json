{
    "env" : {
        "myCodePath": "${workspaceFolder}/src"
    },
    "configurations": [
        {
            "name": "Mac",
            "intelliSenseMode": "clang-x64",
            "includePath": [
                "${myCodePath}",
                "${myCodePath}/lib",
                "${myCodePath}/lib/kernel",
                "${myCodePath}/user"
            ],
            "macFrameworkPath": [ "/System/Library/Frameworks" ],
            "defines": [ "USERPROG" ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c99",
            "cppStandard": "c++11",
            "browse": {
                "path": [ "${workspaceFolder}" ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}